{
    "manifest_version": 2,
    "name": "Word Of The Tab",
    "version": "1.0.1",
    "description": "Shows a random word when creating new tab",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}
